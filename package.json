{
  "name": "prompt-library-abseth",
  "version": "1.0.0",
  "description": "A comprehensive collection of AI prompts with React UI and SQLite",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run load-data && tsc && vite build",
    "build:no-data": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "setup-db": "node scripts/setup-database.js",
    "load-data": "node scripts/load-json-to-db.js",
    "rebuild-db": "npm run load-data && npm run build"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "better-sqlite3": "^9.2.2",
    "prisma": "^6.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "keywords": [
    "prompts",
    "ai",
    "library",
    "react",
    "typescript",
    "sqlite"
  ],
  "author": "Abseth",
  "license": "MIT"
}
